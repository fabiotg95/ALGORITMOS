#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

void cuadratica (float a, float b, float c, float & x1, float & x2);
void main()
{   float a,b,c, comp=0, x1, x2;
	cout<<"digite el valor de a:";
	cin>> a;
	cout<<"digite el valo de b:";
    cin>> b;
	cout<<"digite le valor de c:";
	cin>> c;
	comp=b*b-4*a*c;
	 if(comp>0)
	 {cuadratica(a,b,c,x1,x2);
      cout<<"mostrar el resultado de"<<x1;
	  cout<<"mostrar el resultado de"<<x2;
	 }
	 else
		 cout<<"los resultados no son reales";
	 getch();
}
    
void cuadratica ( float a, float b, float c, float &x1, float &x2);
 {x1=(-b+sqrt(b*b-4*a*c))/2*a);
    x2=(-b-sqrt(b*b-4*a*c))/2*a);
 }



